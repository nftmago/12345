{
  "expo": {
    "name": "AINUT",
    "slug": "ainut-nutrition-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#33B852"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ainut.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#33B852"
      },
      "package": "com.ainut.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AINUT to access your camera to analyze meals."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow AINUT to access your photos to analyze meals."
        }
      ]
    ]
  }
}